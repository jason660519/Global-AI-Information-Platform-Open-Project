{
  "name": "global-ai-info-platform",
  "version": "1.0.0",
  "description": "A comprehensive platform for aggregating and analyzing global AI information",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "crawl": "node src/crawlers/index.js",
    "crawl:github": "node src/crawlers/sources/github-crawler.js",
    "crawl:actions": "node src/index.js",
    "test:actions": "node test-github-actions.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js",
    "export:csv": "node src/exporters/csv-exporter.js",
    "upload:supabase": "node src/uploaders/supabase-uploader.js"
  },
  "keywords": [
    "ai",
    "information",
    "platform",
    "crawler",
    "data-analysis",
    "supabase",
    "github-api"
  ],
  "author": "Global AI Info Platform Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.6.7",
    "cheerio": "^1.1.2",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "joi": "^17.12.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4"
  }
}